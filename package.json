    "eslint-config-dev": "^2.0.0",
    "postcss-tape": "^4.0.0",
    "pre-commit": "^1.2.2",
    "rollup": "^1.7.3",
    "rollup-plugin-babel": "^4.3.2"
  },
  "eslintConfig": {
